<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appnavigation">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppNavigation">
        <activity
            android:name=".navgrap.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".navgrap.AboutActivity" />
        <!--activity
            android:name=".intentflags.SecondActivity"/>
        <activity
            android:name=".intentflags.ThirdActivity"/>
        <activity
            android:name=".intentflags.AboutActivity"/>-->
    </application>

</manifest>